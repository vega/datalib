  "repository": {
    "type": "git",
    "url": "http://github.com/vega/datalib.git"
  },
  "author": {
    "name": "Jeffrey Heer",
    "url": "http://idl.cs.washington.edu"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "d3": "^3.5.x",
    "topojson": "^1.6.18",
    "request": "^2.53.0",
    "sync-request": "^2.0.1"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "browserify-shim": "^3.8.5",
    "chai": "^1.10.0",
    "coveralls": "^2.11.2",
    "gulp": "^3.8.11",
    "gulp-jshint": "^1.10.0",
    "gulp-rename": "^1.2.2",
    "gulp-spawn-mocha": "^2.0.1",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "mocha": "^2.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "gulp test",
    "publish": "scripts/publish.sh"
  },
  "browserify": {
    "transform": [
